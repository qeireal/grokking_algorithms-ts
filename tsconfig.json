{
    "compilerOptions": {
        "baseUrl": "./",
        "declaration": false,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "paths": {
            "@customTypes/*": ["./customTypes/*"],
            "@utils/*": ["./utils/*"]
        },
        "target": "es3",
        "lib": [
            "es6",
            "esnext.bigint"
        ]
    }
}
